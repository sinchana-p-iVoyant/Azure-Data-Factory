{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azdf-ivo-eus-demo"
		},
		"AzureBlobStorage_LS2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage_LS2'"
		},
		"AzureBlobStorage_LinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage_LinkedService'"
		},
		"Destination DataLake_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Destination DataLake'"
		},
		"Destination_LS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Destination_LS'"
		},
		"Source BlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Source BlobStorage'"
		},
		"Source DataLake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Source DataLake'"
		},
		"Source_LS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Source_LS'"
		},
		"sourceDataLake2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'sourceDataLake2'"
		},
		"Files_Source_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Files_Source'"
		},
		"Files_Destination_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Files_Destination'"
		},
		"Source DataLake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageadf3.dfs.core.windows.net/"
		},
		"sourceDataLake2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageadf3.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azdf-ivo-eus-demo_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage_LS2_connectionString": {
						"value": "[parameters('AzureBlobStorage_LS2_connectionString')]"
					},
					"AzureBlobStorage_LinkedService_connectionString": {
						"value": "[parameters('AzureBlobStorage_LinkedService_connectionString')]"
					},
					"Destination DataLake_connectionString": {
						"value": "[parameters('Destination DataLake_connectionString')]"
					},
					"Destination_LS_connectionString": {
						"value": "[parameters('Destination_LS_connectionString')]"
					},
					"Source BlobStorage_connectionString": {
						"value": "[parameters('Source BlobStorage_connectionString')]"
					},
					"Source DataLake_accountKey": {
						"value": "[parameters('Source DataLake_accountKey')]"
					},
					"Source_LS_connectionString": {
						"value": "[parameters('Source_LS_connectionString')]"
					},
					"sourceDataLake2_accountKey": {
						"value": "[parameters('sourceDataLake2_accountKey')]"
					},
					"Files_Source_connectionString": {
						"value": "[parameters('Files_Source_connectionString')]"
					},
					"Files_Destination_connectionString": {
						"value": "[parameters('Files_Destination_connectionString')]"
					},
					"Source DataLake_properties_typeProperties_url": {
						"value": "[parameters('Source DataLake_properties_typeProperties_url')]"
					},
					"sourceDataLake2_properties_typeProperties_url": {
						"value": "[parameters('sourceDataLake2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}